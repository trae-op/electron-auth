generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int        @id @default(autoincrement())
  email    String
  name     String
  picture  String?
  provider Providers?
}

enum Providers {
  google
  facebook
}
